<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/engine/llm/"
     xml:base="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/engine/llm/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cat="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/">

    <owl:Ontology rdf:about="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/engine/llm">
        <rdfs:label>LLM chat engine for Ecosystems</rdfs:label>
        <rdfs:comment>Rails Engine providing multi-provider LLM chat UI with Raix 2.0, </rdfs:comment>
    </owl:Ontology>

    <!-- Repository Individual -->
    <owl:NamedIndividual rdf:about="#EngineLlm">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#RaySwarmEngine"/>
        <cat:repositoryName>engine-llm</cat:repositoryName>
        <cat:repositoryUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/laquereric/engine-llm</cat:repositoryUrl>
        <cat:gemName>engine-llm</cat:gemName>
        <cat:engineNamespace>EngineLlm</cat:engineNamespace>
        <cat:purpose>LLM chat engine for Ecosystems</cat:purpose>
        <cat:techStack>Ruby, Rails Engine</cat:techStack>
        <cat:dependsOn rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/rails/#Rails"/>
        <cat:dependsOn rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/view_component/#ViewComponent"/>
        <cat:dependsOn rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/raix/#Raix"/>
        <cat:provides rdf:resource="#Conversation"/>
        <cat:provides rdf:resource="#HeartBeat"/>
        <cat:provides rdf:resource="#HeartBeatExecution"/>
        <cat:provides rdf:resource="#Preference"/>
        <cat:provides rdf:resource="#Provider"/>
        <cat:provides rdf:resource="#ProviderModel"/>
        <cat:provides rdf:resource="#Setting"/>
    </owl:NamedIndividual>

    <!-- Component: Conversation -->
    <owl:NamedIndividual rdf:about="#Conversation">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>Conversation</rdfs:label>
        <rdfs:comment>Model backed by table: cl_conversations</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Component: HeartBeat -->
    <owl:NamedIndividual rdf:about="#HeartBeat">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>HeartBeat</rdfs:label>
        <rdfs:comment>Model backed by table: cl_heartbeats</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Component: HeartBeatExecution -->
    <owl:NamedIndividual rdf:about="#HeartBeatExecution">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>HeartBeatExecution</rdfs:label>
        <rdfs:comment>Model backed by table: cl_heartbeat_executions</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Component: Preference -->
    <owl:NamedIndividual rdf:about="#Preference">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>Preference</rdfs:label>
        <rdfs:comment>Model backed by table: cl_preferences</rdfs:comment>
        <cat:associatedWith rdf:resource="#DefaultModel"/>
        <cat:associatedWith rdf:resource="#PreferredProvider"/>
    </owl:NamedIndividual>

    <!-- Component: Provider -->
    <owl:NamedIndividual rdf:about="#Provider">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>Provider</rdfs:label>
        <rdfs:comment>Model backed by table: cl_providers</rdfs:comment>
        <cat:associatedWith rdf:resource="#ProviderModels"/>
    </owl:NamedIndividual>

    <!-- Component: ProviderModel -->
    <owl:NamedIndividual rdf:about="#ProviderModel">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>ProviderModel</rdfs:label>
        <rdfs:comment>Model backed by table: cl_provider_models</rdfs:comment>
        <cat:associatedWith rdf:resource="#Provider"/>
    </owl:NamedIndividual>

    <!-- Component: Setting -->
    <owl:NamedIndividual rdf:about="#Setting">
        <rdf:type rdf:resource="http://www.semanticweb.org/ericlaquer/ontologies/2026/1/rayswarm/catalog/#Component"/>
        <rdfs:label>Setting</rdfs:label>
        <rdfs:comment>Model backed by table: cl_settings</rdfs:comment>
    </owl:NamedIndividual>

</rdf:RDF>
